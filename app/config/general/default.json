                    "format": "global",
                    "deps": ["https://www.ebi.ac.uk/gxa/resources/js/lib/fetch-polyfill.min.js", "https://www.ebi.ac.uk/gxa/resources/js-bundles/vendorCommons.bundle.js"]
                }
